{
  "name": "observable-react",
  "version": "1.0.0",
  "main": "dist/observable-react.js",
  "module": "src/observable-react.js",
  "license": "MIT",
  "scripts": {
    "demo:build": "webpack --config ./demos/webpack.config.js",
    "demo:start": "webpack-dev-server --config ./demos/webpack.config.js",
    "build": "webpack",
    "lint": "standard",
    "test": "jest",
    "ci:test": "jest --coverage",
