    "install-peers": "yarn add --peer react react-dom rxjs"
  },
  "jest": {
    "testRegex": "src/.*\\.(spec|test)\\.js$"
  },
  "dependencies": {
    "lodash": "^4.17.5"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.4.2",
    "standard": "^11.0.0",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1"
  },
  "peerDependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "rxjs": "^5.5.6"
  }
}
